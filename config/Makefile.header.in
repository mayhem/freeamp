# Freeamp Makefile header 
# contains all the compiler-related configure-extracted vars
# $Id: Makefile.header.in,v 1.1.2.1 1999/05/17 19:55:26 dogcow Exp $

SHELL = /bin/sh

srcdir = @srcdir@
top_srcdir = @top_srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

bindir = @bindir@
sbindir = @sbindir@
libexecdir = @libexecdir@
datadir = @datadir@
sysconfdir = @sysconfdir@
sharedstatedir = @sharedstatedir@
localstatedir = @localstatedir@
libdir = @libdir@
infodir = @infodir@
mandir = @mandir@
includedir = @includedir@

DESTDIR =

top_builddir = .

AUTOCONF = @AUTOCONF@

INSTALL = @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_SCRIPT = @INSTALL_SCRIPT@
transform = @program_transform_name@

CPU = @host_cpu@

host_alias = @host_alias@
host_triplet = @host@
CC = @CC@
CXX = @CXX@
CXXLD = $(CXX)
GTK_CFLAGS = @GTK_CFLAGS@
GTK_CONFIG = @GTK_CONFIG@
GTK_LIBS = @GTK_LIBS@
MAKEINFO = @MAKEINFO@
NASM = @NASM@
PACKAGE = @PACKAGE@
PERL = @PERL@
RANLIB = @RANLIB@
VERSION = @VERSION@
HOST_OS = @host_os@
CFLAGS = -Wall @CFLAGS@ -D_REENTRANT
OSDEP = @OSDEP@
OSINC = @OSINC@
FPIC = @FPIC@
LINKMOD = @LINKMOD@ $(LDFLAGS)

OSDEPOBJ = @OSDEPOBJ@

INCLUDES = -Ibase/include -Iconfig -Iio/include -Iui/include -Ilmc/include $(OSINC) -Iio/soundcard/$(OSDEP)/include -Iui/lcd/include -Iui/irman/include -Ilmc/xingmp3/include
CXXFLAGS = $(EXTRA_CXXFLAGS)
ACLOCAL_M4 = $(top_srcdir)/aclocal.m4
mkinstalldirs = $(SHELL) $(top_srcdir)/config/mkinstalldirs
CONFIG_HEADER = ./config/config.h
CONFIG_CLEAN_FILES = 
PROGRAMS =  $(bin_PROGRAMS)

DEFS = -I. -I$(srcdir) -I./config -DUNIX_LIBDIR=\"$(libdir)\" -D$(HOST_OS)
CPPFLAGS = @CPPFLAGS@
LDFLAGS = @LDFLAGS@
X_CFLAGS = @X_CFLAGS@
X_LIBS = @X_LIBS@ -lX11 -lXext @XPMLIB@
X_EXTRA_LIBS = @X_EXTRA_LIBS@
X_PRE_LIBS = @X_PRE_LIBS@

FREEAMPOBJ =  base/src/hashtable.o base/src/log.o base/src/player.o
FREEAMPOBJ += base/src/playlist.o base/src/propimpl.o base/src/registrar.o
FREEAMPOBJ += base/src/registry.o base/src/thread.o io/src/eventbuffer.o
FREEAMPOBJ += base/src/prefixprefs.o base/src/preferences.o
FREEAMPOBJ += io/src/pmiregistry.o io/src/pmoregistry.o
FREEAMPOBJ += io/src/pullbuffer.o io/src/streambuffer.o
FREEAMPOBJ += lmc/src/lmcregistry.o ui/src/uiregistry.o
FREEAMPOBJ += $(OSDEPOBJ)

COMPILE = $(CC) $(DEFS) $(INCLUDES) $(CFLAGS)
CXXCOMPILE = $(CXX) $(DEFS) $(INCLUDES) $(CFLAGS) $(CPPFLAGS) $(CXXFLAGS)

CXXLINK = $(CXXLD) $(CXXFLAGS) $(LDFLAGS) -o $@

